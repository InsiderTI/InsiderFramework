{
  "name": "insider_framework",
  "version": "0.2.1",
  "description": "Hybrid framework",
  "main": "init.js",
  "scripts": {
    "babel-node": "babel-node --presets=latest ./init.js",
    "start": "nodemon --exec npm run babel-node",
    "start-with-eslint": "npm run lint && nodemon --exec npm run babel-node",
    "inspect": "npm run lint && pm2 start --inspect' && echo '\n\nOpen in chrome the URL chrome://inspect to debug\n\n'",
    "lint": "eslint init.js && eslint keyclasses/js/*.js && eslint config/*.js"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "eslintIgnore": [
    "node_modules/**/*.js"
  ],
  "dependencies": {
    "body-parser": "^1.18.3",
    "delay": "*",
    "express": "^4.16.3",
    "fs": "*",
    "helmet": "^3.13.0",
    "htmlspecialchars": "^1.0.5",
    "in-array": "^0.1.2",
    "magic-globals": "^0.9.1",
    "md5": "^2.2.1",
    "method-override": "^3.0.0",
    "mkdirp": "*",
    "mysql2": "*",
    "node-cmd": "*",
    "nodemailer": "^4.6.8",
    "rimraf": "*",
    "rtrim": "^1.0.0",
    "svn-spawn": "*",
    "underscore": "^1.9.1",
    "wait-until": "0.0.2"
  },
  "author": "Insider TI",
  "deprecated": false,
  "license": "UNLICENSED",
  "repository": {
    "type": "none",
    "url": "none"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "sync-promise": "*"
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "keywords": [
    "Insider_Framework",
    "Nodejs",
    "BabelJs"
  ]
}
